{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist/employee-rating-app"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/dist/employee-rating-app/index.html"
    }
  ]
}
